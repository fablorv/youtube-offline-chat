{

	"manifest_version": 3,
	"name": "offline chat",
	"version":"0.0.1",
	"cross_origin_embedder_policy": {
	    "value": "require-corp"
	  },
	  "cross_origin_opener_policy": {
	    "value": "same-origin"
	  },

	"web_accessible_resources":[
		{
			"resources" :["/iframe/index.html","/iframe/build/index.html","/iframe/reactframe/dist.index.html","/Untitled.png"],
			"matches":["<all_urls>"]
		}
	],
	"content_scripts":[
		{
			"matches": ["*://*.youtube.com/c/*","*://*.youtube.com/channel/*"],
			"js": ["content.js"],
			"all_frames":true
		}
	]
	
}
