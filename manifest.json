{

	"manifest_version": 3,
	"name": "offline chat",
	"version":"0.0.1",

	"web_accessible_resources":[
		{
			"resources" :["/iframe/index.html","/iframe/build/index.html","/Untitled.png"],
			"matches":["<all_urls>"]
		}
	],
	"content_scripts":[
		{
			"matches": ["*://*.youtube.com/c/*","*://*.youtube.com/channel/*"],
			"js": ["content.js"],
			"all_frames":true
		}
	]
	
}
